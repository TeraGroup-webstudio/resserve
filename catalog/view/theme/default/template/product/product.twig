{{ header }}


<div id="product-product" class="container">
  <ul class="breadcrumb">
    {% for breadcrumb in breadcrumbs %}
    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
    {% endfor %}
  </ul>
  <div class="row">{{ column_left }}
    {% if column_left and column_right %}
    {% set class = 'col-sm-6' %}
    {% elseif column_left or column_right %}
    {% set class = 'col-sm-9' %}
    {% else %}
    {% set class = 'col-sm-12' %}
    {% endif %}
    <div id="content" class="{{ class }}">{{ content_top }}
      <div class="row"> {% if column_left or column_right %}
        {% set class = 'col-sm-6' %}
        {% else %}
        {% set class = 'main-slide' %}
        {% endif %}
        <div class="product__page">
            <div class="product__page-content">
                <ul class="product__navigation">
                    <li><a href="#about_product">{{about_product}}</a></li>
                    {% if description != '' %}<li><a href="#description_product">{{description_product}}</a></li>{% endif %}
                    {% if attribute_groups %}<li><a href="#attributes_product">{{attributes_product}}</a></li>{% endif %}
                    <li><a href="#reviews_product">{{reviews_product}}</a></li>
                </ul>
                <div id="about_product">
                    <div class="ptoduct__images">
                        <div class="productpage__carousel">
                            {% if images %}
                            <div class="product__carousel--thumbs swiper-container">
                                <div class="swiper-wrapper">
                                    <div class="swiper-slide product__carousel--thumb">
                                        <img src="{{ main_thumb }}" title="{{ heading_title }}" class="img-responsive" alt="{{ heading_title }}" />
                                    </div>
                                    {% for image in images %}
                                    <div class="swiper-slide product__carousel--thumb">
                                        <img src="{{ image.thumb }}" title="{{ heading_title }}" class="img-responsive" alt="{{ heading_title }}" />
                                    </div>
                                    {% endfor %}
                                </div>
                            </div>
                            {% endif %}
                            {% if images %}
                            <div class="product__carousel--slides swiper-container">
                                <div class="product__carousel--button swiper-button-prev">
                                    <svg width="28" height="28" viewBox="0 0 28 28" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M19.2 14H8.79995M8.79995 14L13.35 9.44995M8.79995 14L13.35 18.55" stroke="black" stroke-linecap="round" stroke-linejoin="round"/>
                                        <path d="M14 27C6.82036 27 1 21.1796 1 14C1 6.82029 6.82036 1 14 1C21.1797 1 27 6.82029 27 14C27 21.1796 21.1797 27 14 27Z" stroke="black" stroke-linecap="round" stroke-linejoin="round"/>
                                    </svg>
                                </div>
                                <div class="product__carousel--button swiper-button-next">
                                    <svg width="28" height="28" viewBox="0 0 28 28" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M8.80005 14H19.2M19.2 14L14.65 9.44995M19.2 14L14.65 18.55" stroke="black" stroke-linecap="round" stroke-linejoin="round"/>
                                        <path d="M14 27C21.1796 27 27 21.1796 27 14C27 6.82029 21.1796 1 14 1C6.82029 1 1 6.82029 1 14C1 21.1796 6.82029 27 14 27Z" stroke="black" stroke-linecap="round" stroke-linejoin="round"/>
                                    </svg>
                                </div>
                                <div class="swiper-wrapper">
                                    <div class="product__carousel--slide swiper-slide thumbnails">
                                        <a data-fancybox="gallery" href="{{ popup }}">
                                            <img src="{{ main_thumb }}" title="{{ heading_title }}" class="img-responsive" alt="{{ heading_title }}" />
                                        </a>
                                    </div>
                                    {% for image in images %}
                                   <div class="product__carousel--slide swiper-slide thumbnails">
                                        <a data-fancybox="gallery" href="{{ image.popup }}">
                                            <img src="{{ image.main_thumb }}" class="img-responsive" title="{{ heading_title }}" alt="{{ heading_title }}" />
                                        </a>
                                    </div>
                                    {% endfor %}
                                </div>
                            </div>
                            {% endif %}
                        </div>
                        {% if stickers %}
                            <ul class="stickers_image stickers_image_product" style="position: absolute;top: 10px;right: 10px;z-index: 2;left: unset;">
                                {% for sticker in stickers %}
                                    <li><img src="{{ sticker.image }}" title="{{ heading_title }}" alt="{{ heading_title }}" /></li>
                                {% endfor %}
                            </ul>
                        {% endif %}
                    </div>
                </div>
                <div id="description_product">
                    <div class="product__page--title">{{description_product}}</div>
                    {% if product_shot_description_status == 1 and description != '' %}
                        <div class="read__more">
                            <div class="read__more--content">
                                <div class="readmore__block">
                                    <div class="read__more--text">{{ description }}</div>
                                    <div class="read__more--button">
                                      <button>
                                        <svg width="40" height="27" viewBox="0 0 40 27" fill="none" xmlns="http://www.w3.org/2000/svg">
                                          <rect width="38.8889" height="27" rx="4" transform="matrix(-1 0 0 1 39.4445 0)" fill="#40AB35" />
                                          <mask id="path-2-inside-1_10_4918" fill="white">
                                            <path d="M19.4445 3L10.9591 11.4853L19.4445 19.9707L27.9298 11.4853L19.4445 3Z" />
                                          </mask>
                                          <path d="M19.4445 19.9707L20.8587 21.3849L19.4445 22.7991L18.0302 21.3849L19.4445 19.9707ZM12.3733 10.0711L20.8587 18.5564L18.0302 21.3849L9.54492 12.8995L12.3733 10.0711ZM18.0302 18.5564L26.5156 10.0711L29.344 12.8995L20.8587 21.3849L18.0302 18.5564Z" fill="white" mask="url(#path-2-inside-1_10_4918)" />
                                        </svg>
                                      </button>
                                    </div>
                                  </div>
                            </div>
                        </div>
                    {% else %}
                        <div class="product__page--description">description</div>
                    {% endif %}
                </div>
                {% if attribute_groups %}
                <div id="attributes_product">
                    <div class="product__page--title">{{attributes_product}}</div>
                    <div class="product__page--attributes attributemore__block {% if attribute_count >= 8 %} attributemore__block--large {% endif %}">
                        {% for attribute_group in attribute_groups %}
                            <div class="product__page--attribute--name">{{ attribute_group.name }}</div>
                            {% for attribute in attribute_group.attribute %}
                            <div class="product__page--attribute">
                                <div class="product__page--attribute-title">{{ attribute.name }}</div>
                                <div class="product__page--attribute--text">{{ attribute.text }}</div>
                            </div>
                            {% endfor %}
                        {% endfor %}
                    </div>
                    {% if attribute_count >= 8 %}
                    <div class="attribute__more--button">
                        <button>
                          <svg width="40" height="27" viewBox="0 0 40 27" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <rect width="38.8889" height="27" rx="4" transform="matrix(-1 0 0 1 39.4445 0)" fill="#40AB35" />
                            <mask id="path-2-inside-1_10_4918" fill="white">
                              <path d="M19.4445 3L10.9591 11.4853L19.4445 19.9707L27.9298 11.4853L19.4445 3Z" />
                            </mask>
                            <path d="M19.4445 19.9707L20.8587 21.3849L19.4445 22.7991L18.0302 21.3849L19.4445 19.9707ZM12.3733 10.0711L20.8587 18.5564L18.0302 21.3849L9.54492 12.8995L12.3733 10.0711ZM18.0302 18.5564L26.5156 10.0711L29.344 12.8995L20.8587 21.3849L18.0302 18.5564Z" fill="white" mask="url(#path-2-inside-1_10_4918)" />
                          </svg>
                        </button>
                      </div>
                      {% endif %}
                </div>
                {% endif %}
                <div id="reviews_product">
                    
                </div>
            </div>
            <div class="product__page--info" id="product__page--info">
                <h1 data-da=".product__page-content,1023,0">{{ heading_title }}</h1>
                <div class="rating__block" data-da=".product__page-content,1023,1">
                    {% if review_status %}
                        {% if review_guest %}
                            {% if rating %}
                            <div class="single__product--rating-wrapper">
                                <div class="rating">
                                    {% for i in 1..5 %}
                                        {% if rating < i %}
                                            <svg width="11" height="11" viewBox="0 0 34 34" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path d="M16.7258 24.0713L25.3396 29.2702L23.0537 19.4717L30.664 12.8789L20.6424 12.0287L16.7258 2.78766L12.8092 12.0287L2.78763 12.8789L10.3979 19.4717L8.11201 29.2702L16.7258 24.0713Z" fill="#8E9192" />
                                            </svg>
                                        {% else %}
                                            <svg width="11" height="11" viewBox="0 0 34 34" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path d="M16.7258 24.0713L25.3396 29.2702L23.0537 19.4717L30.664 12.8789L20.6424 12.0287L16.7258 2.78766L12.8092 12.0287L2.78763 12.8789L10.3979 19.4717L8.11201 29.2702L16.7258 24.0713Z" fill="#FFAB03" />
                                            </svg>
                                        {% endif %}
                                    {% endfor %}
                                </div>
                                <div class="reviews__count">
                                    <svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M5.92991 1C3.8457 1 1.87728 2.38 1.24044 4.48C0.198332 7.96 2.6878 11.2 5.92991 11.2C6.62465 11.2 7.37728 11.02 8.01412 10.72L10.851 11.2L10.3878 8.26C10.6773 7.6 10.851 6.82 10.851 6.1C10.851 3.28 8.65096 1 5.92991 1Z" stroke="#8E9192" />
                                    </svg>
                                    <div class="reviews__count--numbers">{{reviews}}</div>
                                </div>
                            </div>
                        {% endif %}
                        {% else %}
                        {{ text_login }}
                        {% endif %}
                    {% endif %}
                    {% if product_model_status == 1 %}
                        <div class="model">{{ text_model }} {{ model }}</div>
                    {% endif %}
                </div>
                {% if manufacturer and product_manufacturer_status %}
                    <div class="manufacturer" data-da=".product__page-content,1023,2">{{ text_manufacturer }} <a href="{{ manufacturers }}">{{ manufacturer }}</a></div>
                {% endif %}
                {% if reward %}
                    <div class="reward">
                        {{ text_reward }} <span class="red">{{ reward }}</span>
                        <svg xmlns="http://www.w3.org/2000/svg" width="17" height="17" viewBox="0 0 17 17" fill="none">
                            <path d="M14.8611 8.70068V15.9393C14.8611 16.1945 14.6543 16.4014 14.3991 16.4014H3.00208C2.7469 16.4014 2.54004 16.1945 2.54004 15.9393V8.70068" stroke="#15A74C" stroke-linecap="round" stroke-linejoin="round"/>
