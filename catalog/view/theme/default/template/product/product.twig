                        {{ text_one_click_buy }}
                    </button>
                 {% endif %}
                 <div class="product__buttons">
                    {% if product_compare_status == 1 %}
                        <button type="button" data-toggle="tooltip" title="{{ button_compare }}" onclick="compare.add('{{ product_id }}');">
                            <svg width="15" height="16" viewBox="0 0 15 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <g clip-path="url(#clip0_27_24885)">
                                  <mask id="mask0_27_24885" style="mask-type:luminance" maskUnits="userSpaceOnUse" x="0" y="0" width="15" height="16">
