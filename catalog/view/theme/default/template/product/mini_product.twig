<div class="single__product swiper-slide">

        <div class="single__product--wrapper {% if quantity > 0 %}not_enough{% endif %}">
            {{ viewed }}
        <div class="single__product--image">
            {% if label_new %}
                <div class="label_latest">
                    New
                </div>
            {% endif %}
            {% if label_hit %}
                <div class="label_hits">
                    Hit
                </div>
            {% endif %}
            {% if type_stikers %}
                {% if type_stikers == 'special' %}
                    <div class="label_special">
                        Sale
                    </div>
                {% elseif type_stikers == 'latest' %}
                    <div class="label_latest">
                        New
                    </div>
                {% endif %}
            {% endif %}
            {% if stickers %}
                <ul class="stickers_image stickers_image_product" style="position: absolute;top: 10px;left: 10px;z-index: 2;right: unset;">
                    {% for sticker in stickers %}
                        <li><img src="{{ sticker}}" title="{{ heading_title }}" alt="{{ heading_title }}" /></li>
                    {% endfor %}
                </ul>
            {% endif %}
            <a href="{{ href }}">
                {% if lazy_load_status == 1  and not no_lazy %}
                    <img  data-original="{{ thumb }}" alt="{{ name }}" title="{{ name }}" class="img-responsive lazy-load"/>
                {% else %}
                    <img src="{{ thumb }}" alt="{{ name }}" title="{{ name }}" class="img-responsive" />
                {% endif %}
            </a>
            {% if product_wishlist_status == 1 %}
                <button class="single__product--wishlist" data-toggle="tooltip" title="{{ button_wishlist }}" onclick="wishlist.add('{{ product_id }}');">
                    <svg width="16" height="15" viewBox="0 0 16 15" fill="none" xmlns="http://www.w3.org/2000/svg">
