    }
    .breadcrumb i {
        font-size: 15px;
    }
    .breadcrumb > li {
        list-style-type: none;
        position: relative;
        padding-right: 18px;
        white-space: nowrap;
    }
    .breadcrumb > li + li:before {
        content: '';
        padding: 0;
    }
    .breadcrumb > li:after {
        content: '';
