    .other .head-bottom {
        position: fixed;
        width: 100%;
        left: 0;
        padding-left: 10px;
        padding-right: 10px;
        padding-top: 15px;
        padding-bottom: 15px;
        background: #fff;
        bottom: -100vh;
    }
    header.fixed .head-bottom {
        display: none;
    }
    header.fixed .head-bottom.active{
        display: block;
    }
    span.price-old {
        color: #888;
        font-size: 12px;
        font-style: normal;
        font-weight: 400;
        line-height: 20px;
        text-decoration: line-through;
    }

    span.price-new {
        color: #FF2D55;
        font-size: 15px;
        font-style: normal;
        line-height: 20px;
        margin-left: 11px;
