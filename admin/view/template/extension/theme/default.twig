{{ header }}{{ column_left }}
<div id="content">
    <div class="page-header">
        <div class="container-fluid">
            <div class="pull-right">
                <button type="submit" form="form-theme" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i></button>
                <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a></div>
            <h1>{{ heading_title }}</h1>
            <ul class="breadcrumb">
                {% for breadcrumb in breadcrumbs %}
                    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                {% endfor %}
            </ul>
        </div>
    </div>
    <div class="container-fluid">
        {% if error_warning %}
            <div class="alert alert-danger alert-dismissible"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
                <button type="button" class="close" data-dismiss="alert">&times;</button>
            </div>
        {% endif %}
        {% if start_setting %}
            <div class="well">
                <h4>{{ text_start_setting }}</h4>
                <p class="info-line"><i class="fa fa-info" aria-hidden="true"></i> {{ text_simple_setting }}</p>
                <button type="button" id="start_site" class="btn btn-danger">{{ button_set_preferences }}</button>
            </div>
        {% endif %}
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"><i class="fa fa-pencil"></i> {{ text_edit }}</h3>
            </div>
            <div class="panel-body">
                <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-theme" class="form-horizontal">
                    <div class="row">
                        <div class="vertical-tab-container">
                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2 vertical-tab-menu">
                                <div class="list-group">
                                    <a href="#" class="list-group-item active text-center">
                                        {{ tab_main }}
                                    </a>
                                    <a href="#" class="list-group-item text-center">
                                        {{ tab_header }}
                                    </a>
                                    <a href="#" class="list-group-item text-center">
                                        {{ tab_footer }}
                                    </a>
                                    <a href="#" class="list-group-item text-center">
                                        {{ tab_category }}
                                    </a>
                                    <a href="#" class="list-group-item text-center">
                                        {{ tab_product }}
                                    </a>
                                    <a href="#" class="list-group-item text-center">
                                        {{ tab_image }}
                                    </a>
                                    <a href="#" class="list-group-item text-center">
                                        {{ tab_seo }}
                                    </a>
                                    <a href="#" class="list-group-item text-center">
                                        {{ tab_robots }}
                                    </a>
                                </div>
                            </div>
                            <div class="col-lg-10 col-md-10 col-sm-10 col-xs-10 vertical-tab">
                                <div class="vertical-tab-content active text-center">
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h4 class="panel-title"><i class="fa fa-cogs fw"></i> {{ text_general }}</h4>
                                        </div>
                                        <div class="panel-body">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label" for="input-directory"><span data-toggle="tooltip" title="{{ help_directory }}">{{ entry_directory }}</span></label>
                                                <div class="col-sm-4">
                                                    <select name="theme_default_directory" id="input-directory" class="form-control">
                                                        {% for directory in directories %}
                                                            {% if directory == theme_default_directory %}
                                                                <option value="{{ directory }}" selected="selected">{{ directory }}</option>
                                                            {% else %}
                                                                <option value="{{ directory }}">{{ directory }}</option>
                                                            {% endif %}
                                                        {% endfor %}
                                                    </select>
                                                </div>
                                                <label class="col-sm-2 control-label" for="input-status">{{ entry_status }}</label>
                                                <div class="col-sm-4">
                                                    <div class="btn-group" data-toggle="buttons">
                                                        <label class="btn btn-default btn-danger {% if not theme_default_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_status" value="0" {% if not theme_default_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_off }}</span>
                                                        </label>
                                                        <label class="btn btn-default {% if theme_default_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_status" value="1" {% if theme_default_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_on }}</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h4 class="panel-title"><i class="fa fa-cogs fw"></i> {{ entry_no_index_status }}</h4>
                                        </div>
                                        <div class="panel-body">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label" for="input-status">{{ entry_no_index_status }}</label>
                                                <div class="col-sm-4">
                                                    <div class="btn-group" data-toggle="buttons">
                                                        <label class="btn btn-default btn-danger{% if not theme_default_no_index_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_no_index_status" value="0" {% if not theme_default_no_index_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_off }}</span>
                                                        </label>
                                                        <label class="btn btn-default {% if theme_default_no_index_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_no_index_status" value="1" {% if theme_default_no_index_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_on }}</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h4 class="panel-title"><i class="fa fa-cogs fw"></i> {{ help_webfont_status }}</h4>
                                        </div>
                                        <div class="panel-body">
                                            <div class="form-group">
                                                <label class="col-sm-3">
                                                    {{ entry_webfont_status }}
                                                    <span data-toggle="tooltip" title="{{ help_webfont_status }}"></span>
                                                </label>
                                                <div class="col-sm-9">
                                                    <div class="btn-group" data-toggle="buttons" id="web_font_status">
                                                        <label class="btn btn-default btn-danger{% if not theme_default_webfont_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_webfont_status" value="0" {% if not theme_default_webfont_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_off }}</span>
                                                        </label>
                                                        <label class="web_font_on btn btn-default {% if theme_default_webfont_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_webfont_status" value="1" {% if theme_default_webfont_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_on }}</span>
                                                        </label>
                                                    </div>
                                                    <script>
                                                        $('#web_font_status .btn').click( function(e) {
                                                            var element = $(this), enabled = element.find('[type=radio]').val() == 1;
                                                            setTimeout(function () {
                                                                if (element.hasClass('active') && enabled) {
                                                                    $('#web_fonts').collapse('show');
                                                                } else {
                                                                    $('#web_fonts').collapse('hide');
                                                                }
                                                            }, 10);
                                                        });
                                                    </script>
                                                    <div id="web_fonts" class="collapse fade {% if theme_default_webfont_status %}in{% endif %}">
                                                        <div class="form-group">
                                                            <div class="mb-3">
                                                                <div class="col-sm-12">{{ entry_webfont_link }}</div>
                                                                <div class="col-sm-12">
                                                                    <textarea rows="2" name="theme_default_webfont_link" class="form-control code-field text-monospace" placeholder='<link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">'>{{ theme_default_webfont_link }}</textarea>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <div class="col-sm-12">{{ entry_webfont_style }}</div>
                                                            <div class="col-sm-12">
                                                                <textarea rows="2" name="theme_default_webfont_style" class="form-control code-field text-monospace" placeholder='font-family: "Open Sans", sans-serif;'>{{ theme_default_webfont_style }}</textarea>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h4 class="panel-title"><i class="fa fa-cogs fw"></i> Bootstrap</h4>
                                        </div>
                                        <div class="panel-body">
                                            <label class="col-sm-3 control-label">{{ text_version_bootstrap }}<beta>beta</beta></label>
                                            <div class="col-sm-4">
                                                <select name="theme_default_version_bootstrap" class="form-control">
                                                    <option value="0" {% if theme_default_version_bootstrap == 0  %} selected="selected"{% endif %} disabled="disabled">{{ text_no_bootstrap }}</option>
                                                    <option value="1" {% if theme_default_version_bootstrap == 1  %} selected="selected"{% endif %}>{{ text_v_3_5_bootstrap }}</option>
                                                    <option value="2" {% if theme_default_version_bootstrap == 2  %} selected="selected"{% endif %} disabled="disabled">{{ text_v_4_5_bootstrap }}</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h4 class="panel-title"><i class="fa fa-cogs fw"></i> {{ text_main_setting_dop }}</h4>
                                        </div>
                                        <div class="panel-body">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label" for="input-status">{{ entry_all_product_wishlist_status }}</label>
                                                <div class="col-sm-4">
                                                    <div class="btn-group" data-toggle="buttons">
                                                        <label class="btn btn-default btn-danger{% if not theme_default_all_product_wishlist_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_all_product_wishlist_status" value="0" {% if not theme_default_all_product_wishlist_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_off }}</span>
                                                        </label>
                                                        <label class="btn btn-default {% if theme_default_all_product_wishlist_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_all_product_wishlist_status" value="1" {% if theme_default_all_product_wishlist_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_on }}</span>
                                                        </label>
                                                    </div>
                                                </div>
                                                <label class="col-sm-2 control-label" for="input-status">{{ entry_all_product_compare_status }}</label>
                                                <div class="col-sm-4">
                                                    <div class="btn-group" data-toggle="buttons">
                                                        <label class="btn btn-default btn-danger{% if not theme_default_all_product_compare_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_all_product_compare_status" value="0" {% if not theme_default_all_product_compare_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_off }}</span>
                                                        </label>
                                                        <label class="btn btn-default {% if theme_default_all_product_compare_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_all_product_compare_status" value="1" {% if theme_default_all_product_compare_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_on }}</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label" for="input-status">{{ entry_product_label_new_status }}</label>
                                                <div class="col-sm-2">
                                                    <div class="btn-group" data-toggle="buttons">
                                                        <label class="btn btn-default btn-danger{% if not theme_default_product_label_new_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_product_label_new_status" value="0" {% if not theme_default_product_label_new_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_off }}</span>
                                                        </label>
                                                        <label class="btn btn-default {% if theme_default_product_label_new_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_product_label_new_status" value="1" {% if theme_default_product_label_new_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_on }}</span>
                                                        </label>
                                                    </div>
                                                </div>
                                                <label class="col-sm-2 control-label" for="input-status">{{ entry_product_label_new_day }}</label>
                                                <div class="col-sm-2"><input type="text" class="form-control" name="theme_default_product_label_new_day" value="{{ theme_default_product_label_new_day }}"/></div>
                                                <label class="col-sm-2 control-label" for="input-status">{{ entry_product_label_hit_status }}</label>
                                                <div class="col-sm-2">
                                                    <div class="btn-group" data-toggle="buttons">
                                                        <label class="btn btn-default btn-danger{% if not theme_default_product_label_hit_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_product_label_hit_status" value="0" {% if not theme_default_product_label_hit_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_off }}</span>
                                                        </label>
                                                        <label class="btn btn-default {% if theme_default_product_label_hit_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_product_label_hit_status" value="1" {% if theme_default_product_label_hit_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_on }}</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="vertical-tab-content text-center">
                                    <!----------- header ------------>
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h4 class="panel-title"><i class="fa fa-cogs fw"></i> {{ text_box_information }}</h4>
                                        </div>
                                        <div class="panel-body">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label" for="input-box-information-status">{{ entry_header_box_info }}</label>
                                                <div class="col-sm-2">
                                                    <div class="btn-group" data-toggle="buttons" id="input-box-information-status">
                                                        <label class="btn btn-default btn-danger {% if not theme_default_header_box_info_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_header_box_info_status" value="0" {% if not theme_default_header_box_info_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_off }}</span>
                                                        </label>
                                                        <label class="btn btn-default {% if theme_default_header_box_info_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_header_box_info_status" value="1" {% if theme_default_header_box_info_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_on }}</span>
                                                        </label>
                                                    </div>
                                                </div>
                                                <script>
                                                    $('#input-box-information-status .btn').click( function(e) {
                                                        var element = $(this), enabled = element.find('[type=radio]').val() == 1;
                                                        setTimeout(function () {
                                                            if (element.hasClass('active') && enabled) {
                                                                $('#box-info-header').collapse('show');
                                                            } else {
                                                                $('#box-info-header').collapse('hide');
                                                            }
                                                        }, 10);
                                                    });
                                                </script>
                                            </div>
                                            <div id="box-info-header" class="collapse fade {% if theme_default_header_box_info_status %}in{% endif %}">
                                                <div class="form-group">
                                                    <label class="col-sm-2 control-label" for="header_box_info_status">{{ text_header_box_info_image }}</label>
                                                    <div class="col-sm-2">
                                                        <a href="" id="thumb-image-header-box" data-toggle="image" class="img-thumbnail"><img src="{{ theme_default_header_box_info_thumb }}" alt="" title="" data-placeholder="{{ placeholder }}"/></a>
                                                        <input type="hidden" name="theme_default_header_box_info_image" value="{{ theme_default_header_box_info_image }}" id="input-image-header-box"/>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-2 control-label" for="header_box_info_status">{{ text_header_box_info_text }}</label>
                                                    <div class="col-sm-10">
                                                        {% for language in languages %}
                                                            <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}" />
                                                    </span>
                                                                <textarea name="theme_default_header_box_info_text[{{ language.language_id }}]" class="form-control" cols="40" rows="5" placeholder="">{{ theme_default_header_box_info_text[language.language_id] ? theme_default_header_box_info_text[language.language_id] : '' }}</textarea>
                                                            </div>
                                                        {% endfor %}
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-2 control-label" for="header_box_info_status">{{ text_header_box_info_price }}</label>
                                                    <div class="col-sm-10">
                                                        {% for language in languages %}
                                                            <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}" />
                                                    </span>
                                                                <input type="text" name="theme_default_header_box_info_price_ot[{{ language.language_id }}]" value="{{ theme_default_header_box_info_price_ot[language.language_id] ? theme_default_header_box_info_price_ot[language.language_id] : '' }}" class="form-control">
                                                                <input type="text" name="theme_default_header_box_info_price[{{ language.language_id }}]" value="{{ theme_default_header_box_info_price[language.language_id] ? theme_default_header_box_info_price[language.language_id] : '' }}" class="form-control">
                                                                <input type="text" name="theme_default_header_box_info_price_symbol[{{ language.language_id }}]" value="{{ theme_default_header_box_info_price_symbol[language.language_id] ? theme_default_header_box_info_price_symbol[language.language_id] : '' }}" class="form-control">
                                                            </div>
                                                        {% endfor %}
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-2 control-label" for="header_box_info_status">{{ text_header_box_info_url }}</label>
                                                    <div class="col-sm-10">
                                                        {% for language in languages %}
                                                            <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}" />
                                                    </span>
                                                                <input type="text" name="theme_default_header_box_info_url[{{ language.language_id }}]" value="{{ theme_default_header_box_info_url[language.language_id] ? theme_default_header_box_info_url[language.language_id] : '' }}" class="form-control">
                                                            </div>
                                                        {% endfor %}
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <div class="vertical-tab-content text-center">
                                    <!---------- footer -------------->
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h4 class="panel-title"><i class="fa fa-cogs fw"></i> {{ text_footer_seti }}</h4>
                                        </div>
                                        <div class="panel-body">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label" for="input-footer-header-seti-status">{{ entry_footer_seti_status }}</label>
                                                <div class="col-sm-2">
                                                    <div class="btn-group" data-toggle="buttons" id="input-footer-header-seti-status">
                                                        <label class="btn btn-default btn-danger {% if not theme_default_footer_seti_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_footer_seti_status" value="0" {% if not theme_default_footer_seti_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_off }}</span>
                                                        </label>
                                                        <label class="btn btn-default {% if theme_default_footer_seti_status %} active {% endif %}">
                                                            <input type="radio" name="theme_default_footer_seti_status" value="1" {% if theme_default_footer_seti_status %} checked {% endif %}>
                                                            <span class="on-off">{{ button_on }}</span>
                                                        </label>
                                                    </div>
                                                </div>
                                                <script>
                                                    $('#input-footer-header-seti-status .btn').click( function(e) {
                                                        var element = $(this), enabled = element.find('[type=radio]').val() == 1;
                                                        setTimeout(function () {
                                                            if (element.hasClass('active') && enabled) {
                                                                $('#list_seti').collapse('show');
                                                            } else {
                                                                $('#list_seti').collapse('hide');
                                                            }
                                                        }, 10);
                                                    });
                                                </script>
                                            </div>
                                            <div id="list_seti" class="collapse fade {% if theme_default_footer_seti_status %}in{% endif %}">
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label">{{ entry_social }}</label>
                                                    <div class="col-sm-9">
                                                        <table id="seti-items" class="table table-bordered">
                                                            <thead>
                                                                <tr class="text-center">
                                                                    <td class="nowrap">{{ column_icon }}</td>
                                                                    <td class="nowrap">{{ column_link }}</td>
                                                                    <td class="nowrap">{{ column_sort }}</td>
                                                                    <td class="w-1"></td>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                            {% set item_row_seti = 0 %}
                                                            {% for theme_default_footer_seti in theme_default_footer_seties %}
                                                                <tr id="item-row-seti{{ item_row_seti }}">
                                                                    <td class="text-center">
                                                                        <a href="" id="thumb-image-add-seti-peace-{{ item_row_seti }}" data-toggle="image" class="img-thumbnail"><img src="{{ theme_default_footer_seti.thumb_peace }}" alt="" title="" data-placeholder="{{ placeholder_icon }}"  /></a><input type="hidden" name="theme_default_footer_seti[{{ item_row_seti }}][image_peace]" value="{{ theme_default_footer_seti.image_peace }}" id="input-image-add-seti-peace-{{ item_row_seti }}" />
                                                                    </td>
                                                                    <td class="text-left">
                                                                        {% for language in languages %}
                                                                            <div class="input-group  pull-left">
                                                                                <span class="input-group-addon"><img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}" /></span>
                                                                                <input class="form-control" type="text" name="theme_default_footer_seti[{{ item_row_seti }}][link][{{ language.language_id }}]" value="{{ theme_default_footer_seti.link[language.language_id] }}" />
                                                                            </div>
                                                                        {% endfor %}
                                                                    </td>
                                                                    <td class="text-left">
                                                                        <input  class="form-control" type="text" name="theme_default_footer_seti[{{ item_row_seti }}][sort]" value="{{ theme_default_footer_seti.sort }}" />
                                                                    </td>
                                                                    <td class="text-center">
                                                                        <a class="btn btn-danger" onclick="$('#item-row-seti{{ item_row_seti }}').remove();" data-toggle="tooltip" title="{{ button_delete }}"><i class="fa fa-trash-o"></i></a>
                                                                    </td>
                                                                </tr>
                                                                {% set item_row_seti = item_row_seti + 1 %}
                                                            {% endfor %}
                                                            </tbody>
                                                            <tfoot>
                                                            <tr>
                                                                <td colspan="3"></td>
                                                                <td class="text-center"><a class="btn btn-default" onclick="addItemSeti();" data-toggle="tooltip" title="{{ button_add }}"><i class="fa fa-plus"></i></a></td>
                                                            </tr>
                                                            </tfoot>
                                                        </table>
                                                        <script type="text/javascript">
